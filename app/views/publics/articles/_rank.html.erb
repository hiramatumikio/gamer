<div class="col-md-offset-4 col-md-4 col-sm-6 col-xs-6">
<div class="pull-right">
            <div class="flex5">
            <div class="col-md-3">
              <div class="row">
                <h3><div class="label" style="background-color: #880000; margin: 10px 30px 10px; border-radius: 100px;";>   ランキング</div></h3>
                  <% @all_ranks.each.with_index(1) do |note| %>
                    <div class="col-md-11 " style="background-color: white; margin: 0px 30px 10px; border-radius: 100px;">
                      <div class="col-md-8 text-left">
                        <%= link_to publics_article_path(note.id) do %>
                          <%= attachment_image_tag note, :image, :fill, 1000, 1000, format: 'jpeg', size: '150x150' %>
                        <% end %>
                      </div>
                    </div>
                  <% end %>
              </div>
              </div>
            
          </div>
        </div>
    </div>