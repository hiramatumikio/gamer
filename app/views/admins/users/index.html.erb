<div class="col-xs-9">
  <div class="col-xs-3">
    <%= render 'admins/users/sidebar' %>
  </div>
    <h1>ユーザー管理一覧</h1>
      <h3 class="pull-right">表示数1~10件</h3>

		   <script type="text/javascript">
               $(function() {
                $('h3').css('color','lime');
               });
           </script>

			<table class="table table-bordered">
				<tr class="active">
	              <th>ID</th>
	              <th>ニックネーム</th>
	              <th>メールアドレス</th>
	              <th></th>
	            </tr>
	            <tr class="active">
			   <% @users.each do |user| %>
	              <td class="active"><%= user.id %></td>
	              <td class="active"><%= user.nickname %></td>
	              <td class="active"><%= user.email %></td>
	              <td class="active"><%= link_to"ユーザー情報編集", edit_admins_user_path(user.id) %></td>
	            </tr>
               <% end %>
            </table>
		</div>